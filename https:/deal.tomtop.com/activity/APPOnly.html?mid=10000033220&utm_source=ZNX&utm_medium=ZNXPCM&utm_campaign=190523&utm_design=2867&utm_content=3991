    function compareTime(eTime) {auto
        var utcTime2 = new Date(utcTime);
        utcTime2 = utcTime2.getFullYear() > 0 ? utcTime2 : new Date(Date.parse(utcTime.replace(/-/g, "/")));//时间比较兼容ios
        var tab_startTime = new Date(eTime);
