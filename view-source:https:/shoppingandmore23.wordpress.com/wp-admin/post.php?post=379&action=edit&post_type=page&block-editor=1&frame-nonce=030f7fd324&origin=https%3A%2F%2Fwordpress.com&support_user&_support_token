<script type="text/html" id="tmpl-grunion-field-edit">
	<div class="card is-compact grunion-field-edit grunion-field-{{ data.type }}" aria-label="Formularfeld">
		<label class="grunion-name">
			<span>Feldbezeichnung</span>
			<input type="text" name="label" placeholder="Titel" value="{{ data.label }}"/>
		</label>

				<div class="grunion-type-options">
			<label class="grunion-type">
				Feldtyp				<select name="type">
										<option <# if ( 'text' === data.type ) print( "selected='selected'" ) #> value="text">
						Text					</option>
