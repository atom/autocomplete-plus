	<script type="text/javascript">
	( function() {
		var tmplNode = jQuery( '#tmpl-attachment-details-two-column' );

		if ( ! tmplNode ) {
			return;
		}

		var content = tmplNode.html();
		var target = '<div class="file-size">';

		var targetIndex = content.indexOf( target );

		if ( -1 == targetIndex ) {
			return;
		}

		var insertStringAt = function( stringToInsert, contextString, insertAtIndex ) {
			return [
				contextString.slice( 0, insertAtIndex ),
				stringToInsert,
				contextString.slice( insertAtIndex ),
			].join( '' );
		}
		var conditionToAddOpening = '<# if ( data.filesizeHumanReadable ) { #>';
		var conditionToAddClosing = '<# } #>';
		var endOfTargetLine = content.indexOf( '\n', targetIndex );

		var newContent = insertStringAt( conditionToAddClosing, content, endOfTargetLine );
		newContent = insertStringAt( conditionToAddOpening, newContent, targetIndex );

		tmplNode.html( newContent );
	} )();
</script>
