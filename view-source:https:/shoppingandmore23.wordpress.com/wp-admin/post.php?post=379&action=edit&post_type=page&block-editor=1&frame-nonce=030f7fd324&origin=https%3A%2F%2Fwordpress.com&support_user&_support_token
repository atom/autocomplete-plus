		<script>
			// <![CDATA[
			var tinyMentions = {
				waitForTinyMCE: function() {
					if ( !tinyMentions.checkTiny() ) {
						setTimeout( tinyMentions.waitForTinyMCE, 500 );
						return;
					}
					tinyMentions.attachTiny();
				},
				checkTiny: function() {
					if ( 'undefined' == typeof tinyMCE || ! tinyMCE.activeEditor || ! tinyMCE.activeEditor.contentDocument ) {
						return false;
					}
					return true;
				},
				attachTiny: function() {
					// To the visual editor
					jQuery( tinyMCE.activeEditor.contentDocument.activeElement ).atwho( 'setIframe', jQuery( '#content_ifr' )[0] ).mentions( jetpackMentionsData );
				},
				go: function( mentions ) {
					tinyMentions.waitForTinyMCE();

					var $ed = jQuery( '#content' );
					if ( $ed.length && $ed.is( 'textarea' ) ) {
						$ed.mentions( jetpackMentionsData );
					}
				}
			};
			jQuery( document ).on( 'mentionsData.jetpack', function() {
				tinyMentions.go();
			})
			// ]]>
