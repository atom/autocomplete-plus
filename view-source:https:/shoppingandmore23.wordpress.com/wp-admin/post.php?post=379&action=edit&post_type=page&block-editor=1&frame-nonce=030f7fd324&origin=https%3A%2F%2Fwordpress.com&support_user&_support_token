	<a href='edit.php?post_type=feedback' class="wp-not-current-submenu menu-top menu-icon-feedback" ><div class="wp-menu-arrow"><div></div></div><div class='wp-menu-image svg' style="background-image:url('data:image/svg+xml;base64,')"><br /></div><div class='wp-menu-name'>Feedback</div></a></li>
	<li class="wp-not-current-submenu menu-top toplevel_page_edit?post_type=feedback" id="toplevel_page_edit-post_type-feedback">
	<a href='edit.php?post_type=feedback' class="wp-not-current-submenu menu-top toplevel_page_edit?post_type=feedback" ><div class="wp-menu-arrow"><div></div></div><div class='wp-menu-image dashicons-before dashicons-feedback'><br /></div><div class='wp-menu-name'>Kontaktformular</div></a></li>
	<li class="wp-not-current-submenu menu-top toplevel_page_polls" id="toplevel_page_polls">
	<a href='admin.php?page=polls' class="wp-not-current-submenu menu-top toplevel_page_polls" ><div class="wp-menu-arrow"><div></div></div><div class='wp-menu-image dashicons-before dashicons-chart-bar'><br /></div><div class='wp-menu-name'>Umfragen</div></a></li>
	<li class="wp-not-current-submenu menu-top toplevel_page_ratings menu-top-last" id="toplevel_page_ratings">
	<a href='admin.php?page=ratings' class="wp-not-current-submenu menu-top toplevel_page_ratings menu-top-last" ><div class="wp-menu-arrow"><div></div></div><div class='wp-menu-image dashicons-before dashicons-star-filled'><br /></div><div class='wp-menu-name'>Bewertungen</div></a></li>
	<li class="wp-not-current-submenu wp-menu-separator" aria-hidden="true"><div class="separator"></div></li>
